swiper{
  width:100%;
  height: 340rpx;
}
swiper image{
  width:100%;
  height: 100%;
}
.infoText{
  padding:20rpx;
  border-bottom:2rep solid #eee;
}
.infoText .infoTitle{
  color:#444;
  font-size:30rpx;
  line-height: 2;
}
.infoText .infoDesc{
  color:#666;
  font-size:25rpx;
  line-height: 0.6;
}
.containers{
  margin:20rpx 0;
}
.left{
  width:75%;
  font-size:30rpx;
  float: left;
}
.iconfont{
  font-size:30rpx;
}
.right{
  width:15%;
  float:right;
  text-align: center;
  border-left:1rpx solid #eee;
}
.address{
  margin:0 5rpx;
}
.cate{
  display:flex;
  justify-content: space-around;
  text-align: center;
  margin:0 auto;
}
.cate view.iconfont{
  width:80rpx;
  line-height: 80rpx;
  color:#fff;
  font-size:35rpx;
  border-radius: 50%;
}
.list text{
  color:#000;
  font-size:25rpx;
}
.cate view.icon-diancan{
  background: #D63F1C;
}
.cate view.icon-yuding{
  background: #ED1212;
}
.cate view.icon-waimai-{
  background: #0EC2A2;
}
.cate view.icon-paiduiquhao0101{
  background: #F7E738;
}
.title{
  display: block;
  width:100%;
  margin:20rpx auto;
  text-align: center;
  font-size:35rpx;
  color:#333;
}
.proList{
  display: flex;
  justify-content: space-around;
  flex-wrap:wrap;
}
.proList .lists{
  width:30%;
  min-height:330rpx;
  font-size:30rpx;
  text-align: center;
  margin-bottom:20rpx;
  border:2rpx solid #eee;
}
.proList .lists image{
  width:100%;
  height: 70%;
}
.proList .lists text{
  display: block;
}
