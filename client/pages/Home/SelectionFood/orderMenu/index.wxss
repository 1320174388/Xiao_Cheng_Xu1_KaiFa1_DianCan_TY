  background-color:#f5a623;
  text-align:center;
  font-size:16px;
} 
/*清空购物车  */
.display_show{  
    display: block;
}  
.display_none{  
    display: none;
}
.cart-content {
  width:100%;
  overflow:hidden;
}
.cart-content .tool {
  display:flex;
  line-height:70rpx;
  border-bottom:1px solid #ededed;
  background-color: #f7f7fa;
}
.cart-content .tool .left {
  width:70%;
  height: 70rpx;
  padding-left:15px;
  background-color: #f7f7fa;
  color: #c0bbb5;
  font-size:26rpx;
}
.right-clear {
  width:30%;
  height: 70rpx;
  text-align:left;
  background-color: #f7f7fa;
