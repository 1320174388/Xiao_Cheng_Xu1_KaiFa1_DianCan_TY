.page{
  width: 100%;
  height: 1130rpx;
  background-color: #fff;
}
.content-column{  
  /* height: 600px;   */
  display: flex;  
  flex-direction: column;  
} 
/*logo  */
 .shopInfo{
  width: 100%;
  height: 235rpx;
  background: linear-gradient(to right, #FAD35C , #F79E3C);
}
.shopname{
  width: 88%;
  height: 160rpx;
  border-bottom: 1rpx dashed white;
  margin-left: 40rpx;
  padding-top: 10rpx;
}
.shopname-backcolor{
  width: 20%;
  height: 120rpx;
  margin-top:21rpx;
  background:linear-gradient(#F89E3C , #F76E1E);
  border-radius: 5px;
} 
.context-dian{
  width: 90%;
  color: white;
  font-size: 26rpx;
  padding-left:35rpx;
  padding-top: 22rpx;
  display: block;
}
.context-xiao{
  width: 90%;
  text-align: center;
  color: white;
  font-size: 26rpx;
  display: block;
}
.text-shopname{
  position: relative;
  left:23%;
  top:-50%;
}
.context-shopname{
  color: white;
  font-family:"STHeiti Light ",华文细黑;
  font-size: 32rpx;
}
.notice{
  width: 90%;
  height: 60rpx;
  margin-left: 20rpx;
}
.prom{
  width:6%;
  height: 40rpx;
  position: absolute;
  margin-top: 10rpx;
  color: #fff;
}
.into{
  color: white;
  font-size: 28rpx;
  position: relative;
  left:8%;
}
.setout{
  color: white;
  font-size: 28rpx;
  position: relative;
  left:12%;
}
/* 走马灯-公告 */
.notice {
  position: relative;
  z-index: 999;
  font-size: 14px;
  color: #fff;
  line-height: 70rpx;
  display: flex;
  flex-direction: row;
}
/* 公告小图标 */
.notice-left {
  width:80rpx;
}
.notice-icon {
  color: #fff;
  width: 50rpx;
  padding-left: 30rpx;
}
/* 公告内容 */
.notice-middle {
  width: 100%;
  padding-left: 30rpx;
  font-size: 30rpx;
  color: #fff;
}
.notice .notice-middle swiper {
  height: 60rpx;
}

/* 左侧栏 */
.content-row{  
  display: flex;  
  flex-direction: row;  
}  
.nav{
  background-color:#FFF;
}
::-webkit-scrollbar {
  width: 0;
  height: 0;
  color: transparent;
}
/*左侧  */
.content-scroll-left{
  max-height:800rpx;
  background-color:#FFF;
}
.swiper-tab {
  width: 100%;
  text-align: center;
  line-height: 88.7rpx;
  background-color:#f5f5f5;
}

.swiper-tab-list {
  font-size: 30rpx;
  display: inline-block;
  width: 100%;
  border-bottom: 1rpx solid rgba(0, 0, 0, 0.04);
}

.on {
  background-color:#FFF;
  color: #434343;
  border-left: 5rpx solid #da7c0c;
}
/*右侧  */
.content-scroll-right{
  max-height:800rpx;
}
.context-title{
  color: #777;
  font-size: 25rpx;
}
.food-list{
  width: 91.8%;
  height: 180rpx;
  border-bottom: 1rpx solid rgba(0, 0, 0, 0.05);
  margin-left: 35rpx;
}
.food-img{
  width: 120rpx;
  height: 120rpx;
  padding-top: 35rpx;
  border-radius:8rpx;
}
 .context-name{
  width: 72%;
  position: relative;
  top:-125rpx; 
  left:150rpx;   
  font-size: 30rpx;
  color: #646464;
}
.context-price{
  width: 72%;
  position: relative;
  top:-124rpx;
  left:145.5rpx; 
  font-size: 30rpx;
  color: #F64257;
}
.foot-right{
  width: 23%;
  position: relative;
  left: 76%;
  top:-82%;
  flex:1;
  display:flex;
  flex-direction:row;
  justify-content:flex-end;
  align-items:center;
  padding-right:10rpx;
  font-weight: lighter;

}
.iconfont{
  font-family:"iconfont" !important;
  font-size:16px;
  font-style:normal;
  -webkit-font-smoothing:antialiased;
  -moz-osx-font-smoothing:grayscale;
}
.sign{
  font-size: 40rpx;
  color:#f79f51; 
}
 .count{
  font-size:28rpx;
  margin:0 20rpx;
  font-weight:bold;
}
/*购物车  */
 .flex-shop{
  z-index: 2000;
  position:fixed;
  top:77%;
  width:100%;
  background-color:#fff;
} 
.cart-wrap {
  position:fixed;
  top:91.7%;
