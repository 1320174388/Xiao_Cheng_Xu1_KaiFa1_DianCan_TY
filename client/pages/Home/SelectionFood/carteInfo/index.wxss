  align-items:center;
 
}
.shopcar{
  width: 25%;
  height: 50rpx;
  color: #fff;
  font-size:30rpx;
  background: linear-gradient(to right, #FAD35C , #F76b1C);
  text-align: center;
}
.circle1 {
    width: 2%;
    height: 48.4rpx;
    border-radius: 100% 0 0 100%/50%;
    border-right: none;
    background: #FAD35C;
}
.circle2 {
    width: 2%;
    height: 48.4rpx;
    background: #F76b1C;
    border-radius: 0 100% 100% 0/50%;
    border-top: none;
}

/*购物车  */
 .flex-shop{
  z-index: 2000;
  position:fixed;
  top:77%;
  width:100%;
  background-color:#fff;
} 
.cart-wrap {
  position:fixed;
  top:91.7%;
  background-color:#fff;
  border-top: 1rpx solid rgba(0, 0, 0, 0.05);
  color:#fff;
  width:100%;
  line-height:50px;
}
.cart-wrap .gouwuche {
  width:150px;
}
.cart-wrap .gouwuche .gouwuche-wrap {
  width:45px;
  height:45px;
  text-align:center;
  background-color:#dcdcdc;
  border-radius:50%;
