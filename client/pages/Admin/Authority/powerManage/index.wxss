/*背景色  */
page {
    background: #F6F6F6;
}
/*提示框  */
input{
  float: right;
  margin-top: 13px;
  margin-right: 80px;
}
/* 列表布局 */
.tunnel{
    margin-top: 40rpx;
    height: 110rpx;
    width: 100%;
    background: #FFF;
    padding-left: 20px;
}

.uploader-text, .tunnel-text {
    width: 100%;
    line-height: 45px;
    font-size: 35rpx;
    color: #007AFF;
    font-family:"LiHei Pro Medium ",儷黑 Pro;
}

.tunnel-text {
    color: #222;
    border-bottom: 1px solid rgba(0, 0, 0, .1);
}
/*下拉选项  */
.sp{
  position: absolute;
  right:-470rpx;
  font-size: 25px;
  color:gainsboro;
}

/*权限选择  */
.pow{
   margin-top: 30rpx;
}
/* 权限选择字体样式 */
.pow-tx{
  padding-left: 20px;
  font-size: 12px;
  color: Gray;
}
/*权限选择  */
/*店铺管理  */
.tunne2 {
    margin-top: 10rpx;
    height: 110rpx;
    width: 100%;
     background: #FFF;
    display: flex;
    align-items: center;
    font-family:"LiHei Pro Medium ",儷黑 Pro;
}
/*用户、菜单、订单管理  */
.tunne3 {
    height: 120rpx;
    width: 100%;
    background: #FFF;
    border-bottom: 1px solid rgba(0, 0, 0, .1);
    display: flex;
    align-items: center;
    font-family:"LiHei Pro Medium ",儷黑 Pro;
}
.uploader-text2, .tunnel-text2 {
    width: 100%;
    line-height: 50px;
    font-size: 35rpx;
    color: #222;
    font-family:"LiHei Pro Medium ",儷黑 Pro;
    padding-left: 20px;
}
.tunnel-text2 {
    position: relative;
    color: #222;
    display: flex;
    flex-direction: row;
    align-content: center;
    justify-content: space-between;
    box-sizing: border-box;
    border-top: 1px solid rgba(0, 0, 0, .1);
}
.tunnel-switch {
    position: absolute;
    right: 30rpx;
    top: -2rpx;
}
/*确定  */
.tunne4{
    margin-top: 100%;
    height: 50px;
    width: 100%;
    
}
.sure{
    color: #FFF;
    line-height: 50px;
    font-size: 35rpx;
    font-family:"LiHei Pro Medium ",儷黑 Pro;
    text-align: center;
}
.but{
  background:#77b3ff;
  color: #FFF;
}
