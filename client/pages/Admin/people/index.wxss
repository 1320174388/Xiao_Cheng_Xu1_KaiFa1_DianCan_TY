/* pages/Admin/people/index.wxss */
page{
  background: #eee;
}
button{
  display: inline-block;
  margin:0;
  background: #fff;
}
.people{
  width:95%;
  margin:30rpx auto;
  max-height: 250rpx;
  background: #fff;
}
.people .left{
  width:75%;
  float:left;
  padding-top:10%;
}
.people text{
  display: block;
  line-height: 1.3;
  padding-left:10rpx;
}
.people text.name{
  color:#000;
}
.people text.quanxian{
  color:#666;
}
.people .right{
  width:25%;
  float:right;
}
.people .right button{
  margin-bottom:-10rpx;
}
.det{
  position: fixed;
  left:0;
  bottom:0;
  right:0;
  color:#77B3FF;
}